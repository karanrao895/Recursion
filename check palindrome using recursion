#include<bits/stdc++.h>
using namespace std;
bool palindrome(string &s,int i){
    //base case
    if(i>=s.size()/2) return true;
    
    
    //recursive case
    if(s[i]!=s[s.size()-i-1]) return false;
    return palindrome(s,i+1);
}
int main(){
    string s;
    cin>>s;
   if(palindrome(s,0)) cout<<"string is palindrome"<<endl;
   else cout<<"not palindrome"<<endl;
}
