#include <iostream>
using namespace std;

void reversearray(int *a,int n,int s,int e){
    //base case
    if(s>=e){
        return;
    }
    
    //recursive case
    swap(a[s],a[e]);
    reversearray(a,n,s+1,e-1);
}
int main() {
	// your code goes here
	int n;cin>>n;
	int a[100];
	for(int i=0;i<n;i++){
	    cin>>a[i];
	}
	
	reversearray(a,n,0,n-1);
	for(int i=0;i<n;i++){
	    cout<<a[i]<<" ";
	}
	
	
	
	return 0;
}
